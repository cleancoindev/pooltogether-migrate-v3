eMutability\":\"nonpayable\",\"type\":\"function\"},{\"inputs\":[{\"internalType\":\"contract IERC777\",\"name\":\"token\",\"type\":\"address\"},{\"internalType\":\"address\",\"name\":\"to\",\"type\":\"address\"}],\"name\":\"withdrawERC777\",\"outputs\":[],\"stateMutability\":\"nonpayable\",\"type\":\"function\"}],\"devdoc\":{\"details\":\"Note that V3 tickets need to be transferred to this contract for liquidity.\",\"kind\":\"dev\",\"methods\":{\"constructor\":{\"params\":{\"_poolDaiPod\":\"The V2 Pool Dai pod token\",\"_poolDaiToken\":\"The V2 Pool Dai token\",\"_poolUsdcPod\":\"The V2 Pool USDC pod token\",\"_poolUsdcToken\":\"The V2 Pool USDC token\",\"_v3Token\":\"The V3 Pool Dai token\"}},\"owner()\":{\"details\":\"Returns the address of the current owner.\"},\"renounceOwnership()\":{\"details\":\"Leaves the contract without owner. It will not be possible to call `onlyOwner` functions anymore. Can only be called by the current owner. NOTE: Renouncing ownership will leave the contract without an owner, thereby removing any functionality that is only available to the owner.\"},\"tokensReceived(address,address,address,uint256,bytes,bytes)\":{\"params\":{\"amount\":\"The amount of tokens that were sent\",\"from\":\"The user who sent the tokens\",\"to\":\"The recipient of the tokens\"}},\"transferOwnership(address)\":{\"details\":\"Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner.\"},\"withdrawERC20(address,address)\":{\"params\":{\"token\":\"The ERC20 token whose entire balance should be transferred to the owner\"}},\"withdrawERC721(address,uint256,address)\":{\"params\":{\"id\":\"The id of the token to transfer\",\"token\":\"The ERC721 to withdraw\"}},\"withdrawERC777(address,address)\":{\"params\":{\"token\":\"The ERC777 token whose entire balance should be transferred to the owner.\"}}},\"title\":\"Allows no-slippage swaps from PoolTogether V2 tickets to V3\",\"version\":1},\"userdoc\":{\"events\":{\"Rec